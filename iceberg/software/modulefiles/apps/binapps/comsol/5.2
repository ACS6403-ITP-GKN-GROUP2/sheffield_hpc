#%Module10.2#####################################################################

## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        global comsolver

        puts stderr " Makes COMSOL Multiphysics $comsolver available to the system."
}

set     comsolver     5.2
set     comsolroot    /usr/local/packages6/comsol/comsol52/multiphysics/

module-whatis   "sets the necessary COMSOL Multiphysics $comsolver paths"

prepend-path PATH $comsolroot/bin/
setenv _JAVA_OPTIONS "-Xmx1g"

