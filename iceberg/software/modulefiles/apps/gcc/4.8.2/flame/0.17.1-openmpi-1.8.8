## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        global version

        puts stderr "   Makes Flame (xparser) Version 0.17.1 available for use"
}

module-whatis   "Makes Flame (xparser) Version 0.17.1 available"

# module variables

set     version 0.17.1

if { [ module-info mode load] } {

	puts stderr "NOTE: the Flame module also loads the gcc-4.8.2 compilerand OpenMPI-1.8.8 library"
}

#  dependencies
 prepend-path PATH /usr/local/packages6/apps/gcc/4.8.2/flame/0.17.1-openmpi-1.8.8/libtool/2.4.6/bin
 prepend-path PATH /usr/local/packages6/apps/gcc/4.8.2/flame/0.17.1-openmpi-1.8.8/libmboard/0.3.1/bin
 prepend-path PATH /usr/local/packages6/apps/gcc/4.8.2/flame/0.17.1-openmpi-1.8.8/xparser/0.17.1

# module dependencies
module load compilers/gcc/4.8.2
module load mpi/gcc/openmpi/1.8.8

