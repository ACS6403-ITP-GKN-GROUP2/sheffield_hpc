#%Module1.0#####################################################################
##
## LAMMPS module file
##

## Module file logging
source /usr/local/etc/module_logging.tcl
##

proc ModulesHelp { } {
        puts stderr "Adds LAMMPS 22_08_2018 to your environment"
	puts stderr "and loads compiler gcc/4.9.4 & openmpi/2.0.1"
}

module-whatis   "Makes LAMMPS 22_08_2018 available"

if { [ module-info mode load ] } {
    puts stderr "NOTE: the LAMMPS module also loads the gcc-4.9.4 compiler & openmpi/2.0.1"
}

module load mpi/openmpi/2.0.1/gcc-4.9.4

prepend-path PATH /usr/local/packages/apps/lammps/22_08_2018/gcc-4.9.4/bin/
